{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "dom",
      "es2015",
      "es2016",
      "es2017"
    ],
    "allowJs": true,
    "jsx": "react-jsx",
    "sourceMap": true,
    "outDir": "./dist",
    "strict": true ,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "@utils/*": ["src/front/main/utils/*"],
      "@context/*": ["src/front/main/context/*"],
      "@overlay/*": ["src/front/main/overlay/*"],
      "@content/*": ["src/front/main/components/content/*"],
      "@layout/*": ["src/front/main/components/layout/*"],
    }
  }
}
